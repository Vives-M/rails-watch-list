<h1>List of all lists</h1>
<ul>

<% @lists.each do |list| %>
<li class="list-group-item list-group-item-info text-center" ><%= link_to list.name, list_path(list), class:"text-decoration-none text-reset fw-semibold"  %></li>
<% end %>

</ul>

<button class="btn btn-secondary"> <%= link_to "Add a list", new_list_path %> </button>
